<div id="gs-group-member-active" role="region" 
     aria-labelledby="gs-group-member-active-h">
  <h3 id="gs-group-member-active-h">Recently active</h3>
  <a id="gs-group-member-active-all-members" class="header-link"
     tal:define="c view/membersInfo/fullMemberCount;"
     tal:condition="python:c&gt;1"
     tal:attributes="href string:${view/groupInfo/relativeURL}/members.html;
                     title string:;"
     href="members.html">All <span tal:content="c">n</span> members</a>
  <div id="gs-group-member-active-ajax-hole">
  </div><!--gs-group-member-active-ajax-hole-->
  <ul id="gs-group-member-active-stats" class="dotted-inline-list"
      role="navigation">
    <li>
      <a tal:attributes="href string:${view/groupInfo/relative_url}/stats.html"
         href="stats.html">Posting statistics</a>
    </li>
    <li>
      <a tal:attributes="href string:${view/groupInfo/relative_url}/log.html"
         href="log.html">Membership statistics</a>
    </li>
  </ul><!--gs-group-member-active-stats-->
</div><!--gs-group-member-active-->
